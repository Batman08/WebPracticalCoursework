<h1 class="text-center mb-5">My Class Bookings</h1>

{{#user}}
    <div class="row pb-4">
        <div class="col-md-4 mx-auto">
            <div class="card dash-tiled shadow-sm h-100">
                <div class="card-body align-items-start gap-3">
                    <div class="mb-3">
                        <label class="form-label fw-bold" for="inputTitle">Booking Reference</label>
                        <input type="text" class="form-control" id="inputTitle" name="title" value="Course 3 - updated" placeholder="Course title" maxlength="256" required="">
                    </div>

                    <div class="text-center">
                        <button class="btn btn-success shadow-sm"><i class="fa-solid fa-magnifying-glass"></i> Find booking(s)</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    {{^danceClasses}}
        <div class="or-container text-muted">
            <div class="line-separator"></div>
            <div class="or-label">OR</div>
            <div class="line-separator"></div>
        </div>

        <div class="row">
            <div class="col-md-4 mx-auto">
                <button class="btn btn-outline-primary btn-lg shadow-sm mt-4 w-100"><i class="fa fa-right-to-bracket"></i> Log in</button>
            </div>
        </div>
    {{/danceClasses}}

    {{#danceClasses}}
        <div class="row g-4 pb-4">
            <div class="col-md-12">
                  <div class="card shadow-sm h-100">
                    <div class="card-body align-items-start gap-3">
                        <div class="mb-3">
                            <div>{{title}}</div>
                        </div>

                        <div class="mb-3">
                            <div>{{description}}</div>
                        </div>

                        <div class="mb-3">
                            <div>{{classDateTime}}</div>
                        </div>

                        <div class="mb-3">
                            <div>{{duration}}</div>
                        </div>

                        <div class="mb-3">
                            <div>{{location}}</div>
                        </div>

                        <div class="mb-3">
                            <div>{{price}}</div>
                        </div>

                        <div class="text-end mt-3">
                            <button class="btn btn-danger shadow-sm" data-bs-toggle="modal" data-bs-target="#divCreateClassModal"><i class="fa-solid fa-ban"></i> Cancel booking</button>
                        </div>
                    </div>
                  </div>
            </div>
        </div>
    {{/danceClasses}}
{{/user}}

